{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "55"
        },
        "modules": false
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "babel-plugin-kotlish-also",
    "@babel/plugin-syntax-dynamic-import",
    "babel-plugin-styled-components",
    [
      "import",
      {
        "libraryName": "antd",
        "libraryDirectory": "es",
        "style": true
      }
    ]
  ]
}